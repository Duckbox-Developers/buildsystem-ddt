# <file system>	<mount point>	<type>	<options>					<dump>	<pass>
#/dev/root	/		auto	defaults,errors=remount-rw,noatime,nodiratime	0	0
proc		/proc		proc	defaults					0	0
sysfs		/sys		sys	defaults					0	0
tmpfs		/media		media	defaults					0	0
tmpfs		/tmp		tmpfs	defaults					0	0
/dev/mmcblk0p11	/swap		auto	defaults,errors=remount-rw,noatime,nodiratime	0	0
/dev/mmcblk0p10	none 		swap	sw
